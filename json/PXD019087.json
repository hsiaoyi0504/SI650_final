uments": [{"accession": "MS:1002732", "name": "Orbitrap Fusion Lumos"}], "keywords": [{"accession": "MS:1001925", "name": "submitter keyword", "value": "Mouse transposon silencing"}], "modifications": [{"accession": "MS:1002864", "name": "No PTMs are included in the dataset"}], "publications": [{"terms": [{"accession": "MS:1000879", "name": "PubMed identifier", "value": "32719317"}, {"accession": "MS:1002866", "name": "Reference", "value": "Sch\u00f6pp T, Zoch A, Berrens RV, Auchynnikava T, Kabayama Y, Vasiliauskait\u0117 L, Rappsilber J, Allshire RC, O'Carroll D. TEX15 is an essential executor of MIWI2-directed transposon DNA methylation and silencing. Nat Commun. 2020 11(1):3739"}]}], "species": [{"terms": [{"accession": "MS:1001469", "name": "taxonomy: scientific name", "value": "Mus musculus (Mouse)"}, {"accession": "MS:1001467", "name": "taxonomy: NCBI TaxID", "value": "10090"}]}], "title": "TEX15 is an essential executor of MIWI2-directed transposon DNA methylation and silencing."}