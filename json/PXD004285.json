ns": [{"terms": [{"accession": "MS:1000879", "name": "PubMed identifier", "value": "27311716"}, {"accession": "PRIDE:0000400", "name": "Reference", "value": "Campbell SJ, Stern DB. Activation of an Endoribonuclease by Non-intein Protein Splicing. J Biol Chem. 2016 291(31):15911-15922"}]}], "species": [{"terms": [{"accession": "MS:1001469", "name": "taxonomy: scientific name", "value": "Chlamydomonas reinhardtii"}, {"accession": "MS:1001467", "name": "taxonomy: NCBI TaxID", "value": "3055"}]}], "title": "Non-intein splicing of RB47"}