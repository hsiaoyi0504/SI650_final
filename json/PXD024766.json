struments": [{"accession": "MS:1002416", "name": "Orbitrap Fusion"}], "keywords": [{"accession": "MS:1001925", "name": "submitter keyword", "value": "Gutathionylation, Cardiomyocytes, Click chemistry"}], "modifications": [{"accession": "MOD:00394", "name": "acetylated residue"}, {"accession": "MOD:00425", "name": "monohydroxylated residue"}], "publications": [{"terms": [{"accession": "MS:1000879", "name": "PubMed identifier", "value": "34382403"}, {"accession": "MS:1002866", "name": "Reference", "value": "Yapa Abeywardana M, Samarasinghe KTG, Munkanatta Godage D, Ahn YH. Identification and Quantification of Glutathionylated Cysteines under Ischemic Stress. J Proteome Res. 2021"}]}], "species": [{"terms": [{"accession": "MS:1001469", "name": "taxonomy: scientific name", "value": "Mus musculus (Mouse)"}, {"accession": "MS:1001467", "name": "taxonomy: NCBI TaxID", "value": "10090"}]}], "title": "Site-specific Identification and Quantification of Protein Glutathionylation under Ischemic Stress"}